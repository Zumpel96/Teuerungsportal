@page "/search/{searchString}"

<PageTitle>Teuerungsportal - @L?["uncategorizedProducts"]</PageTitle>

<MudGrid>
    <MudItem xs="12">
        <MudText Typo="Typo.h1">@L?["search"]</MudText>
        <MudDivider/>
    </MudItem>

    <MudItem xs="12">
        <MudPaper>
            <RecentPriceChanges PriceChanges="@ProductList" Page="@Page" NumberOfPages="@NumberOfPages" IsLoading="@IsLoading" PageChanged="@OnPageChanged" HideDate/>
        </MudPaper>
    </MudItem>
</MudGrid>